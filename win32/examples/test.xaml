<Page Name='thePage' xmlns='http://schemas.microsoft.com/winfx/2006/xaml/presentation' 
    xmlns:x='http://schemas.microsoft.com/winfx/2006/xaml'  xmlns:m='using:moxaml'>
    <Grid Name='theGrid'>
	<Grid.ColumnDefinitions>
        	<ColumnDefinition Width="*"/>
        	<ColumnDefinition Width="*"/>
    	</Grid.ColumnDefinitions>
	<Grid.RowDefinitions>
        	<RowDefinition Height="*"/>
    	</Grid.RowDefinitions>
     	<Image Grid.Column ="0" Grid.Row="0" m:mox.Event='Click' Name='theImage' Source='https://littlemole.neocities.org/uioo.jpg' />
     	<Image Grid.Column ="1" Grid.Row="0" m:mox.Event='Click' Name='theImage2' Source='https://littlemole.neocities.org/uioo.jpg' />
    </Grid>
</Page>
